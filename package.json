{
  "name": "mern-app",
  "version": "1.0.0",
  "scripts": {
    "gdocs": "cd server && cd gdocs-api && concurrently \"node functions.js\" \"node index.js\"",
    "start-backend": "cd server && npm start",
    "start": "concurrently \"npm run start-backend\" \"npm run react-dev\"",
    "react-dev": "cd client && npm start",
    "test": "cd server && npm test",
    "preinstall": "cd server && npm install",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material/icon-button": "^14.0.0",
    "@mui/lab": "^5.0.0-alpha.79",
    "@mui/material": "^5.6.4",
    "concurrently": "^7.0.0",
    "express": "^4.17.1",
    "googleapis": "^95.0.0",
    "material-ui-icons": "^1.0.0-beta.36",
    "material-ui-search-bar": "^1.0.0",
    "react-router-dom": "^6.2.2",
    "use-between": "^1.3.1"
  },
  "devDependencies": {
    "prettier": "2.5.1"
  }
}
